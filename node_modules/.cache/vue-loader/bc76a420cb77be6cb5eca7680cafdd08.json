{"remainingRequest":"/home/mulama/Desktop/Management/mgt2/node_modules/vue-loader/lib/index.js??vue-loader-options!/home/mulama/Desktop/Management/mgt2/src/views/Sites/medicalCoverExternal.vue?vue&type=script&lang=js&","dependencies":[{"path":"/home/mulama/Desktop/Management/mgt2/src/views/Sites/medicalCoverExternal.vue","mtime":1628485261045},{"path":"/home/mulama/Desktop/Management/mgt2/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/mulama/Desktop/Management/mgt2/node_modules/babel-loader/lib/index.js","mtime":315532800000},{"path":"/home/mulama/Desktop/Management/mgt2/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/home/mulama/Desktop/Management/mgt2/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["medicalCoverExternal.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAwhBA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"medicalCoverExternal.vue","sourceRoot":"src/views/Sites","sourcesContent":["<template>\n  <base-header class=\"pb-6 pb-8 pt-5 pt-md-8 bg-gradient-primary\">\n    <div align-v=\"center\" slot=\"header\">\n      <b-col cols=\"12\" class=\"d-flex justify-content-center mb-3\">\n        <img src=\"img/brand/Blue-Octagon_Logo.png\" style=\"width: 200px; height: 70px\" />\n      </b-col>\n    </div>\n    <br />\n    <b-row class=\"justify-content-center\">\n      <b-col class=\"col-md-7\">\n        <div class=\"d-flex justify-content-center alert alert-info\">\n          <h3>Hello! Let us get to know more about you.</h3>\n        </div>\n      </b-col></b-row\n    >\n    <br />\n    <form-wizard @onComplete=\"completeForm\">\n      <tab-content title=\"Personal Details\" :selected=\"true\">\n        <div class=\"tab\">\n          <div align-v=\"center\" slot=\"header\">\n            <b-col cols=\"12\" class=\"d-flex justify-content-center mb-3\">\n              <strong>Personal Details(Principal Member)</strong>\n            </b-col>\n          </div>\n          <div class=\"row\">\n            <div class=\"col-sm-4\">\n              <div class=\"form-group\">\n                <label class=\"control-label\">Full Names *</label>\n                <input\n                  type=\"text\"\n                  name=\"member_name\"\n                  required\n                  id=\"m_name\"\n                  class=\"form-control\"\n                  v-model=\"payloadData.name\"\n                />\n              </div>\n            </div>\n\n            <input type=\"hidden\" name=\"member_id\" class=\"form-control\" />\n            <div class=\"col-sm-4\">\n              <div class=\"form-group\">\n                <label class=\"control-label\">Email * </label>\n                <input\n                  type=\"email\"\n                  name=\"member_email\"\n                  id=\"m_email\"\n                  class=\"form-control\"\n                  required\n                  v-model=\"payloadData.email\"\n                />\n              </div>\n            </div>\n\n            <!-- <div class=\"col-sm-3\">\n                        \n                                <div class=\"form-group\">\n                                    <label class=\"control-label\">Place of Work/Business</label>\n                                \n                                    <input type=\"text\"  name=\"member_workplace\"  id=\"m_payroll_number\"   class=\"form-control\"  />\n                                </div>\n                            </div> -->\n            <div class=\"col-sm-4\">\n              <div class=\"form-group\">\n                <label class=\"control-label\">Phone Number *</label>\n                <input\n                  type=\"text\"\n                  name=\"member_tel_no\"\n                  id=\"phone_number\"\n                  class=\"form-control\"\n                  required\n                  v-model=\"payloadData.phone\"\n                />\n              </div>\n            </div>\n          </div>\n\n          <div class=\"row\">\n            <div class=\"col-sm-6\">\n              <div class=\"form-group\">\n                <label class=\"control-label\">ID Number *</label>\n                <input\n                  type=\"text\"\n                  name=\"member_id_no\"\n                  id=\"m_id_number\"\n                  class=\"form-control\"\n                  required\n                  v-model=\"payloadData.IDNumber\"\n                />\n              </div>\n            </div>\n\n            <div class=\"col-sm-6\">\n              <div class=\"form-group\">\n                <label class=\"control-label\">Date of Birth *</label>\n                <input\n                  type=\"date\"\n                  name=\"member_dob\"\n                  id=\"from-datepicker\"\n                  placeholder=\"YYYY-MM-DD\"\n                  class=\"form-control inputdate\"\n                  required\n                  v-model=\"payloadData.dob\"\n                />\n              </div>\n            </div>\n\n            <!-- <div class=\"col-sm-3\">\n                                <div class=\"form-group\">\n                                    <label class=\"control-label\">KRA PIN</label>\n                                    <input type=\"text\" name=\"member_kra_pin\" id=\"m_pin\" class=\"form-control\"  >\n                                 </div>\n                                \n                            </div>-->\n          </div>\n        </div>\n      </tab-content>\n      <tab-content title=\"Dependants Information\">\n        <div class=\"tab\">\n          <div class=\"row\" style=\"margin-top: 30px\">\n            <div class=\"col-sm-12\">\n              <h3><label class=\"control-label\">Dependants</label><br /></h3>\n            </div>\n            <div class=\"col-sm-9\">\n              <p class=\"mb20\">\n                Fill out the table below to register the dependants. Use the \"+\" and \"-\"\n                button to add and remove beneficiaries respectively.\n              </p>\n            </div>\n            <div class=\"col-sm-3\">\n              <a class=\"btn btn-info float-right\" v-on:click=\"addRow1()\"\n                ><i class=\"fa fa-plus\"></i\n              ></a>\n            </div>\n          </div>\n\n          <div class=\"row\">\n            <div class=\"col-md-12\">\n              <table class=\"table table-bordered table-hover\">\n                <thead>\n                  <tr>\n                    <th style=\"width: 50px\"></th>\n                    <th class=\"text-center\" style=\"width: 250px\">Dendants Name</th>\n                    <th class=\"text-center\" style=\"width: 200px\">\n                      Relationship to Member\n                    </th>\n                    <th class=\"text-center\" style=\"width: 100px\">Telephone Number</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(ben, index) in payloadData.dependants\">\n                    <td>\n                      <a class=\"btn btn-danger\" v-on:click=\"deleteRow1(index)\"\n                        ><i class=\"fa fa-minus\"></i\n                      ></a>\n                    </td>\n                    <td>\n                      <input type=\"text\" class=\"form-control\" v-model=\"ben.name\" />\n                    </td>\n                    <td>\n                      <base-input>\n                        <select v-model=\"ben.relation\" class=\"form-control\">\n                          <option>Spouse</option>\n                          <option>Son</option>\n                          <option>Daughter</option>\n                        </select>\n                      </base-input>\n                    </td>\n                    <td>\n                      <input type=\"text\" class=\"form-control\" v-model=\"ben.contact\" />\n                    </td>\n                  </tr>\n                  <tr id=\"ben1\"></tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </tab-content>\n      <tab-content title=\"Your Quote \">\n        <div class=\"tab\">\n          <div class=\"row\" style=\"margin-top: 30px\">\n            <div class=\"col-sm-12\">\n              <h3><label class=\"control-label\"></label><br /></h3>\n            </div>\n          </div>\n          <b-row\n            ><b-col class=\"col-md-12\">\n              <div class=\"col-xl-12 alert alert-info\">\n                <h4>Select your preffered Plan</h4>\n              </div>\n            </b-col></b-row\n          >\n          <b-row>\n            <b-col class=\"col-md-12\">\n              <div class=\"table-responsive\">\n                <table class=\"table table-bordered mb30\">\n                  <thead></thead>\n\n                  <tbody v-for=\"i in serviceProvider\">\n                    <tr>\n                      <td>\n                        <strong>{{ i.name }}</strong>\n                      </td>\n                      <td>kshs {{ i.amount }} per month</td>\n                      <td>\n                        <b-form-radio\n                          v-model=\"payloadData.selectedServiceProvider\"\n                          :aria-describedby=\"ariaDescribedby\"\n                          :value=\"i\"\n                        >\n                        </b-form-radio>\n                      </td>\n                    </tr>\n                  </tbody>\n                </table>\n              </div>\n            </b-col>\n          </b-row>\n        </div>\n      </tab-content>\n      <tab-content title=\"Best Plan for your quote\">\n        <div class=\"tab\">\n          <b-row\n            ><b-col class=\"col-md-12\">\n              <div class=\"col-xl-12 alert alert-info\">\n                <h4>Here is the carefully crafted package for you</h4>\n              </div>\n            </b-col></b-row\n          >\n          <div class=\"row\">\n            <div class=\"col-sm-6\">\n              <div class=\"panel panel-\">\n                <div class=\"panel-heading text-center\">\n                  <h3 class=\"panel-title\">Your Quote</h3>\n                </div>\n                <div\n                  class=\"row\"\n                  style=\"border-bottom: 1px solid #eee; height: 10px\"\n                ></div>\n                <div class=\"panel-body\">\n                  <p style=\"color: red\">\n                    <i class=\"fa fa-check\" style=\"color: red\"></i> &nbsp;&nbsp;&nbsp;\n                    Hospital Limit- Ksh 400,000\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Pre-Existing Limit - Ksh 200,000\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Out-patient Limit - Ksh 50,000\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Maternity Limit\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Waiting Period - 3 Months\n                  </p>\n                </div>\n                <!-- panel-body -->\n              </div>\n              <!-- panel -->\n            </div>\n            <div class=\"col-sm-4\">\n              <div class=\"panel panel-primary\">\n                <div class=\"panel-heading text-center\">\n                  <h3 style=\"color: red\"><b>Policy Plan</b></h3>\n                </div>\n                <div\n                  class=\"row\"\n                  style=\"border-bottom: 1px solid #eee; height: 10px\"\n                ></div>\n                <div class=\"panel-body\">\n                  <br /><br />\n                  <h2 style=\"color: red\">\n                    <i class=\"fa fa-check\" style=\"color: red\"></i> Ksh\n                    {{ annual }} Annually\n                  </h2>\n                  <br />\n                  <span class=\"badge badge-pill badge-danger\" v-if=\"verifyCoupon\"\n                    >Invalid coupon code!</span\n                  >\n                  <base-input\n                    type=\"text\"\n                    label=\"Have Coupon Code?\"\n                    placeholder=\"\"\n                    aria-readonly=\"\"\n                    v-model=\"payloadData.couponcode\"\n                  >\n                  </base-input>\n                  <base-button type=\"primary\" v-on:click=\"verifyCopon\"\n                    >Apply Coupon</base-button\n                  >\n                </div>\n                <!-- panel-body -->\n              </div>\n              <!-- panel -->\n            </div>\n          </div>\n        </div>\n      </tab-content>\n      <tab-content title=\"Payment\">\n        <div class=\"tab\">\n          <b-row\n            ><b-col class=\"col-md-12\">\n              <div class=\"col-xl-12 alert alert-success\">\n                <h4>Policy Paymet Details</h4>\n              </div>\n            </b-col></b-row\n          ><br />\n          <b-row>\n            <div class=\"col-sm-6\">\n              <div class=\"panel panel-\">\n                <div class=\"panel-heading text-center\">\n                  <h3 class=\"panel-title\">Your Quote Summary</h3>\n                </div>\n                <div\n                  class=\"row\"\n                  style=\"border-bottom: 1px solid #eee; height: 10px\"\n                ></div>\n                <div class=\"panel-body\">\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Policy Plan Class:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Family Size:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Gross Premium:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Discount:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Discounted Premium:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Insurance Training Levy:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Stamp Duty:\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Premium Payable:\n                  </p>\n                </div>\n                <!-- panel-body -->\n              </div>\n              <!-- panel -->\n            </div>\n            <div class=\"col-sm-6\">\n              <div class=\"panel panel-\">\n                <div class=\"panel-heading text-center\">\n                  <h3 class=\"panel-title\" style=\"color: red\">Payment Procedure</h3>\n                </div>\n                <div\n                  class=\"row\"\n                  style=\"border-bottom: 1px solid #eee; height: 10px\"\n                ></div>\n                <div class=\"panel-body\">\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i>\n                    &nbsp;&nbsp;&nbsp;Lipa na M-PESA > Buy Goods And services 'till no' =\n                    920829\n                  </p>\n                  <p>\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> &nbsp;&nbsp;&nbsp;\n                    Cheque:\n                  </p>\n                  <h2 style=\"color: green\">\n                    <i class=\"fa fa-check\" style=\"color: green\"></i> Ksh\n                    {{ annual }} Annually\n                  </h2>\n                </div>\n                <!-- panel-body -->\n              </div>\n              <!-- panel -->\n            </div>\n          </b-row>\n        </div>\n\n        <div class=\"row\">\n          <div class=\"col-md-12\">\n            <table class=\"table table-bordered table-hover\" id=\"pay_logic\">\n              <thead></thead>\n              <tbody>\n                <tr>\n                  <td><strong>Amount To be Paid</strong></td>\n                  <td>\n                    <input\n                      type=\"text\"\n                      readonly\n                      name=\"payment_amount[]\"\n                      id=\"payment_due0\"\n                      class=\"form-control\"\n                      v-model=\"annual\"\n                    />\n                  </td>\n                </tr>\n                <tr>\n                  <td><strong> Mode of Payment </strong></td>\n                  <td>\n                    <input\n                      type=\"text\"\n                      readonly\n                      id=\"mode_of_payment\"\n                      class=\"form-control\"\n                      v-model=\"payloadData.mode_of_payment\"\n                    />\n                  </td>\n                </tr>\n\n                <tr>\n                  <td><strong> Mpesa phone number(+254) *</strong></td>\n                  <td>\n                    <input\n                      type=\"text\"\n                      name=\"mpesa_phone_number\"\n                      id=\"mpesa_phone_number\"\n                      required\n                      class=\"form-control\"\n                      v-model=\"payloadData.mpesa_phone\"\n                    />\n                  </td>\n                </tr>\n\n                <tr>\n                  <td><strong> Mpesa Code *</strong></td>\n                  <td>\n                    <input\n                      type=\"text\"\n                      name=\"payment_ref[]\"\n                      id=\"payment_ref\"\n                      required\n                      class=\"form-control\"\n                      v-model=\"payloadData.mpesa_code\"\n                    />\n                  </td>\n                </tr>\n\n                <tr></tr>\n                <tr id=\"pay1\"></tr>\n              </tbody>\n            </table>\n          </div>\n        </div>\n        <br />\n        <b-row class=\"d-flex justify-content-center mb-3\">\n          <base-button v-b-modal.modal-1 type=\"success\">Confirm Payment</base-button>\n          <b-modal id=\"modal-1\" title=\"Payment Details\" size=\"lg\" hide-footer>\n            <b-col xl=\"12\" md=\"10\">\n              <stats-card title=\"\" type=\"gradient-green\" sub-title=\"\" class=\"mb-4\">\n                <template slot=\"footer\">\n                  <div class=\"mt-3\">\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Enter Phone No</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"\"\n                          v-model=\"payloadData.mpesa_phone\"\n                        ></b-form-input>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Ref No(Cheque no/Mpesa Code)</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"\"\n                          v-model=\"payloadData.mpesa_code\"\n                        ></b-form-input>\n                      </div>\n                    </div></div></template\n              ></stats-card>\n              <b-row>\n                <div class=\"col-md-4\">\n                  <base-button\n                    class=\"mt-3\"\n                    block\n                    type=\"success\"\n                    @click=\"$bvModal.hide('modal-1')\"\n                    >Save</base-button\n                  >\n                </div> </b-row\n              ><br />\n            </b-col>\n          </b-modal>\n          <base-button type=\"info\">\n            <router-link\n              :to=\"{\n                name: 'register',\n                params: {\n                  details: {\n                    p_name: p_name,\n                    p_email: p_email,\n                  },\n                },\n              }\"\n              >Save and Pay Later</router-link\n            ></base-button\n          >\n        </b-row>\n        <br />\n        <b-row>\n          <b-col lg=\"12\">\n            <div v-loading=\"true\" id=\"loading\" v-if=\"checking\">\n              <p>Submitting request, please wait...</p>\n            </div>\n          </b-col>\n        </b-row>\n        <b-row>\n          <div class=\"justify-content-lg-center\">\n            <span class=\"badge badge-pill badge-success\" v-if=\"success\">\n              Request submitted successfully! <br />kindly be patiant as we process your\n              policy certificate.<br />\n              In the mean time, kindly check your email for your login credentials\n              generated by our system.\n            </span>\n          </div>\n        </b-row>\n        <br />\n      </tab-content>\n    </form-wizard>\n  </base-header>\n</template>\n<script>\nimport { FormWizard, TabContent } from \"vue-step-wizard\";\nimport \"vue-step-wizard/dist/vue-step-wizard.css\";\nimport httpClient from \"../../api/httpClient\";\nimport insuranceErrors from \"../../service/insuranceErrors\";\n\nexport default {\n  name: \"BasicStepperForm\",\n  components: {\n    FormWizard,\n    TabContent,\n  },\n  data() {\n    return {\n      checking: false,\n      success: false,\n      payloadArray: [],\n      payloadData: {\n        selectedProduct: [],\n        name: \"\",\n        email: \"\",\n        phone: \"\",\n        IDNumber: \"\",\n        dob: Date,\n        mpesa_code: \"\",\n        mpesa_phone: \"\",\n        mode_of_payment: \"MPESA\",\n        couponcode: \"\",\n        selectedServiceProvider: [],\n        dependants: [\n          {\n            name: \"\",\n            relation: \"\",\n            contact: \"\",\n          },\n        ],\n      },\n      verifyCoupon: false,\n      serviceProvider: [\n        { name: \"Kenya orient\", amount: \"999\" },\n        { name: \"Pioneer Insurance\", amount: \"4600\" },\n        { name: \"CIC\", amount: \"5600\" },\n      ],\n    };\n  },\n  created() {\n    this.payloadData.selectedProduct.push(this.$route.params.code);\n  },\n  computed: {\n    annual() {\n      return insuranceErrors.calcMedCoverQuote(\n        this.payloadData.selectedServiceProvider.amount\n      );\n    },\n  },\n  methods: {\n    async completeForm() {\n      //this.payloadArray.push(this.payloadData);\n      //console.log(\"medical cover payload\", this.payloadArray);\n\n      var stringjson = JSON.stringify(this.payloadData);\n      let response = \"\";\n      this.checking = true;\n      try {\n        response = await httpClient.post(\"/users\", stringjson);\n        console.log(\"easy cover\", response);\n        console.log(\"payload data\", this.payloadData);\n        this.checking = false;\n      } catch (e) {\n        this.checking = false;\n        this.success = false;\n        console.log(\"Easy cover error\", e);\n      }\n\n      this.checking = false;\n      this.success = false;\n      if (response.status === 200) {\n        this.success = true;\n      }\n    },\n    verifyCopon() {\n      this.verifyCoupon = true;\n    },\n    select() {\n      for (let i in this.serviceProvider) {\n        this.payloadData.selectedServiceProvider.push(this.serviceProvider[i]);\n      }\n    },\n\n    addRow1: function () {\n      this.payloadData.dependants.push({\n        name: \"\",\n        relation: \"\",\n        contact: \"\",\n      });\n    },\n    deleteRow1(index) {\n      this.payloadData.dependants.splice(index, 1);\n    },\n  },\n};\n</script>\n"]}]}