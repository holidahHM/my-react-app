  <b-row class=\"float-right pb-4\">\n        <b-col xl=\"12\">\n          <router-link to=\"/user_rights\">\n            <b-button variant=\"primary\">Add a User</b-button>\n          </router-link>\n        </b-col>\n      </b-row>\n      <div>\n        <b-form>\n          <b-row>\n            <div class=\"my-auto align-self-center\">\n              <b-col xl=\"12\">\n                <h4>VIEW EXISTING USERS</h4>\n              </b-col>\n            </div>\n          </b-row>\n          <br />\n          <b-row>\n            <div class=\"alert alert-danger\" v-if=\"errorSearch\">\n              {{ errorSearch }}\n            </div>\n            <div></div>\n            <div class=\"mx-auto\">\n              <b-col xl=\"12\" sm=\"12\">\n                <p class=\"mb-0\">\n                  <b>Type something in the input field to search the table</b>\n                </p>\n                <b-form-input type=\"search\" label=\"\" placeholder=\"Search for a member\">\n                  <br />\n                </b-form-input>\n              </b-col>\n            </div>\n          </b-row>\n          <br />\n          <b-row>\n            <b-col lg=\"12\" sm=\"12\" xl=\"12\">\n              <div style=\"overflow: scroll\">\n                <b-row>\n                  <table class=\"table table-stripped\">\n                    <b-thead class=\"thead-light\">\n                      <b-tr>\n                        <b-th scope=\"col\">\n                          <b-label class=\"form-checkbox\">\n                            <input type=\"checkbox\" v-model=\"selectAll\" @click=\"select\" />\n                          </b-label>\n                        </b-th>\n                        <b-th scope=\"col\">UserId</b-th>\n                        <b-th scope=\"col\">Full Name</b-th>\n                        <b-th scope=\"col\">Operation level</b-th>\n                        <b-th scope=\"col\">Administration Rights</b-th>\n                        <b-th scope=\"col\">System Group</b-th>\n                        <b-th scope=\"col\">Status</b-th>\n                        <b-th scope=\"col\">Action</b-th>\n                      </b-tr>\n                    </b-thead>\n                    <b-tbody>\n                      <b-tr v-for=\"i in items\">\n                        <b-td>\n                          <label class=\"form-checkbox\">\n                            <input\n                              type=\"checkbox\"\n                              :value=\"i.user_id\"\n                              v-model=\"selected\"\n                            />\n                            <i class=\"form-icon\"></i>\n                          </label>\n                        </b-td>\n                        <b-td>{{ i.user_id }}</b-td>\n                        <b-td>{{ i.user_full_names }}</b-td>\n                        <b-td>{{ i.user_schemes }}</b-td>\n                        <b-td>{{ i.user_admin }}</b-td>\n                        <b-td>{{ i.user_group }}</b-td>\n                        <b-td>{{ i.user_active }}</b-td>\n\n                        <b-td>\n                          <b-dropdown\n                            id=\"dropdown-left\"\n                            text=\"Action\"\n                            variant=\"primary\"\n                            class=\"m-2\"\n                          >\n                            <b-dropdown-item v-b-modal.modal-1\n                              >Edit Details\n                            </b-dropdown-item>\n                            <b-dropdown-item v-b-modal.modal-2\n                              >Reset Password</b-dropdown-item\n                            >\n                          </b-dropdown>\n                        </b-td>\n                      </b-tr>\n                    </b-tbody>\n                  </table>\n                  <div>\n                    <b-pagination\n                      v-model=\"currentPage\"\n                      :total-rows=\"rows\"\n                      :per-page=\"perPage\"\n                      first-number\n                    ></b-pagination>\n                  </div>\n                </b-row>\n              </div>\n            </b-col>\n          </b-row>\n          <b-modal\n            id=\"modal-1\"\n            size=\"lg\"\n            title=\"Edit Existing Member details\"\n            hide-footer\n          >\n            <b-col xl=\"12\" md=\"10\">\n              <stats-card\n                title=\"BIO-DATA\"\n                type=\"gradient-green\"\n                sub-title=\"\"\n                class=\"mb-4\"\n              >\n                <template slot=\"footer\">\n                  <div class=\"mt-3\">\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Index No</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"..Index no\"\n                          readonly\n                        ></b-form-input>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">User ID</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"..User ID\"\n                          readonly\n                        ></b-form-input>\n                      </div>\n                    </div>\n                    <br />\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">User's Name</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"..Member's name\"\n                        ></b-form-input>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">User's Email</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"..Member's Email\"\n                        ></b-form-input>\n                      </div>\n                    </div>\n                    <br />\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">National ID No</label>\n                        <b-form-input type=\"text\" placeholder=\"..ID No\"></b-form-input>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Payroll No:</label>\n                        <b-form-input\n                          type=\"text\"\n                          placeholder=\"..Payroll No\"\n                        ></b-form-input>\n                      </div>\n                    </div>\n                  </div>\n                </template>\n              </stats-card>\n              <stats-card\n                title=\"USER-ACCESS RIGHTS\"\n                type=\"gradient-green\"\n                sub-title=\"\"\n                class=\"mb-4\"\n              >\n                <template slot=\"footer\">\n                  <div class=\"mt-3\">\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Country</label>\n                        <select class=\"form-control\">\n                          <option>Kenya</option>\n                          <option>Uganda</option>\n                          <option>Zambia</option>\n                        </select>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Operation level</label>\n                        <select class=\"form-control\">\n                          <option>Internal</option>\n                          <option>External</option>\n                        </select>\n                      </div>\n                    </div>\n                    <br />\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Administration Rights</label>\n                        <select class=\"form-control\">\n                          <option>Admin-Sys</option>\n                          <option>Admin-Department</option>\n                          <option>Approver</option>\n                          <option>Supervisior</option>\n                          <option>Member</option>\n                        </select>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Access Module</label>\n                        <select class=\"form-control\">\n                          <option>Master-Setup</option>\n                          <option>BD</option>\n                          <option>Onboarding</option>\n                          <option>Servicing</option>\n                          <option>CRM</option>\n                          <option>E-learning</option>\n                          <option>Accounting</option>\n                          <option>Governance</option>\n                          <option>Insurance</option>\n                          <option>Elections</option>\n                          <option>Reports</option>\n                          <option>Document centre</option>\n                          <option>Communication</option>\n                          <option>E-board</option>\n                          <option>Fund Management</option>\n                        </select>\n                      </div>\n                    </div>\n                    <br />\n                    <div class=\"row\">\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">System Group</label>\n                        <select class=\"form-control\">\n                          <option>Member</option>\n                          <option>Normal</option>\n                          <option>HR</option>\n                          <option>Trustee</option>\n                          <option>Service Provideer</option>\n                          <option>None</option>\n                        </select>\n                      </div>\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Delagate Owner</label>\n                        <select class=\"form-control\">\n                          <option>Self</option>\n                          <option>Test-1</option>\n                          <option>Test-2</option>\n                        </select>\n                      </div>\n                    </div>\n                    <br />\n                    <div class=\"row\">\n                      <div class=\"col-xl-12 col-md-6\">\n                        <label for=\"input-default\">Scheme</label>\n                        <div class=\"row\">\n                          <div class=\"col-md-6 col-sm-6\">\n                            <b-form-input\n                              type=\"text\"\n                              placeholder=\"Scheme config\"\n                            ></b-form-input>\n                          </div>\n                          <div class=\"col-md-6 col-sm-6\">\n                            <b-button v-b-modal.modal-3 variant=\"primary\"\n                              >Manage Schemes</b-button\n                            >\n                          </div>\n                        </div>\n                        <br />\n                      </div>\n                      <br />\n                      <div class=\"col-md-6\">\n                        <label for=\"input-default\">Status</label>\n                        <select class=\"form-control\">\n                          <option>Active</option>\n                          <option>Disabled</option>\n                        </select>\n                      </div>\n                    </div>\n                  </div>\n                  <br />\n                </template>\n              </stats-card>\n              <base-button\n                class=\"mt-3\"\n                block\n                @click=\"$bvModal.hide('modal-1')\"\n                type=\"success\"\n                >Save</base-button\n              >\n            </b-col>\n          </b-modal>\n\n          <b-modal id=\"modal-2\" size=\"lg\" title=\"Password Reset\" hide-footer>\n            <stats-card title=\"Email\" type=\"gradient-green\" sub-title=\"\" class=\"mb-4\">\n              <template slot=\"footer\">\n                <div class=\"mt-3\">\n                  <div class=\"row\">\n                    <div class=\"col-md-12 col-sm-12\">\n                      <label for=\"input-default\">User's Email Address</label>\n                      <b-form-input\n                        type=\"text\"\n                        placeholder=\"..Email Address\"\n                        readonly\n                      ></b-form-input>\n                    </div>\n                  </div>\n                  <br />\n                  <div class=\"row\">\n                    <div class=\"col-md-4\">\n                      <b-button variant=\"primary\">Confirm Email Address</b-button>\n                      <br />\n                    </div>\n                    <div class=\"col-md-4\">\n                      <b-button variant=\"primary\" v-on:click=\"isHidden = !isHidden\"\n                        >Change Email Address\n                      </b-button>\n                    </div>\n                    <br />\n                  </div>\n                  <div v-if=\"isHidden\" class=\"row\">\n                    <div class=\"col-md-12 col-sm-12\">\n                      <br />\n                      <label for=\"input-default\">New Email Address</label>\n                      <b-form-input\n                        type=\"text\"\n                        placeholder=\"Enter New Email Address\"\n                      ></b-form-input>\n                      <br />\n                    </div>\n                    <div class=\"col-md-12 col-sm-12\">\n                      <label for=\"input-default\">Confirm Email Address</label>\n                      <b-form-input\n                        type=\"text\"\n                        placeholder=\"Confirm New Email Address\"\n                      ></b-form-input>\n                      <br />\n                    </div>\n                    <div class=\"col-md-4\">\n                      <b-button variant=\"primary\">Save New Email Address </b-button>\n                    </div>\n                  </div>\n                </div>\n                <base-button\n                  class=\"mt-3\"\n                  block\n                  @click=\"$bvModal.hide('modal-2')\"\n                  type=\"success\"\n                  >Generate Password Reset link</base-button\n                >\n              </template>\n            </stats-card>\n          </b-modal>\n          <b-modal id=\"modal-3\" size=\"lg\" title=\"Manage Schemes\" hide-footer> </b-modal>\n        </b-form>\n      </div>\n    </card>\n  </base-header>\n</template>\n<script>\nimport { getAllUsers, searchUser } from \"../../../api/users.api\";\nimport BaseInput from \"../../../components/Inputs/BaseInput.vue\";\nimport { debounce } from \"lodash\";\n\nexport default {\n  components: { BaseInput },\n  data() {\n    return {\n      filter: \"\",\n      perPage: 1,\n      currentPage: 1,\n      i_f_name: \"\", //i stands for internal\n      i_l_name: \"\",\n      i_email: \"\",\n      searchUsername: \"\",\n      user: \"\",\n      keyword: \"\",\n      adminLabel: \"\",\n      activeStatus: \"\",\n      otherUserLabel: \"\",\n      triggerLabel: false,\n      isHidden: false,\n      errorSearch: \"\",\n      user_zone: \"\",\n      user_accessto: \"\",\n      b_division: [\"Default\"], // Array reference\n      lists: [\n        { value: \"Default\", text: \"You haven't choosen any value\", disabled: true },\n        { value: \"Pension\", text: \"Pension\" },\n        { value: \"Insurance\", text: \"Insurance\" },\n        { value: \"Trust\", text: \"Trust\" },\n        { value: \"Acturial\", text: \"Acturial\" },\n        { value: \"Training\", text: \"Training\" },\n      ],\n      items: [],\n      selected: [],\n      selectAll: false,\n      debounceName: \"\",\n    };\n  },\n  created() {\n    this.debounceName = debounce(this.searchLogic, 1000);\n  },\n  computed: {\n    user_Name: function () {\n      return this.i_f_name + \".\" + this.i_l_name;\n    },\n    rows() {\n      return this.items.length;\n    },\n  },\n  methods: {\n    updateProfile() {\n      alert(\"Your data: \" + JSON.stringify(this.user));\n    },\n    async searchLogic() {\n      let response = \"\";\n      this.errorSearch = \"\";\n      try {\n        response = await searchUser(this.keyword);\n        this.items = response.data.data;\n      } catch (e) {\n        this.errorSearch = e.response.message;\n      }\n    },\n    select() {\n      this.selected = [];\n      if (!this.selectAll) {\n        for (let i in this.items) {\n          this.selected.push(this.items[i].user_id);\n        }\n      }\n    },\n  },\n\n  async mounted() {\n    if (localStorage.getItem(\"viewUsers\") != null) {\n      this.items = JSON.parse(localStorage.getItem(\"viewUsers\"));\n    }\n    try {\n      let response = \"\";\n      response = await getAllUsers();\n      console.log(response.data);\n      this.items = response.data.data;\n      localStorage.setItem(\"viewUsers\", JSON.stringify(response.data.data));\n    } catch (e) {}\n    for (let l in this.items) {\n      if (l.user_admin != 0) {\n        this.adminLabel = \"Other User\";\n      } else {\n        this.adminLabel = \"Admin\";\n      }\n    }\n    for (let l in this.items) {\n      if (l.user_active != 1) {\n        this.activeStatus = \"Inactive\";\n      } else {\n        this.activeStatus = \"Active\";\n      }\n    }\n  },\n  watch: {\n    keyword() {\n      if (!this.keyword) return;\n      this.debounceName;\n    },\n  },\n};\n</script>\n<style></style>\n"]}]}