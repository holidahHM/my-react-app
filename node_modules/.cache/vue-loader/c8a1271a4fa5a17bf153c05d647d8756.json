www.w3.org/2000/svg\">\n          <polygon class=\"fill-default\" points=\"2560 0 2560 100 0 100\"></polygon>\n        </svg>\n      </div>\n    </div>\n    <!-- Page content -->\n    <b-container class=\"mt--8 pb-5\">\n      <!-- Table -->\n      <b-row class=\"justify-content-center\">\n        <b-col lg=\"6\" md=\"8\" >\n          <b-card no-body class=\"bg-secondary border-0\">\n            <b-card-header class=\"bg-transparent pb-5\">\n            \n              <div class=\"text-center\">\n                <a href=\"#\" class=\"btn btn-neutral btn-icon mr-4\">\n                  <img src=\"img/brand/Octagon_logo.png\">\n                  \n                </a>\n                \n              </div>\n            </b-card-header>\n            <b-card-body class=\"px-lg-5 py-lg-5\">\n              \n              <validation-observer v-slot=\"{handleSubmit}\" ref=\"formValidator\">\n                <b-form role=\"form\" method=\"post\" @submit.prevent=\"handleSubmit(onSubmit)\">\n                  <base-input alternative\n                              class=\"mb-3\"\n                              prepend-icon=\"ni ni-hat-3\"\n                              placeholder=\"Name\"\n                              name=\"Name\"\n                              :rules=\"{required: true}\"\n                              v-model=\"model.username\">\n                  </base-input>\n\n                  <base-input alternative\n                              class=\"mb-3\"\n                              prepend-icon=\"ni ni-email-83\"\n                              placeholder=\"Email\"\n                              name=\"Email\"\n                              :rules=\"{required: true, email: true}\"\n                              v-model=\"model.user_email\">\n                  </base-input>\n\n                  <base-input alternative\n                              class=\"mb-3\"\n                              prepend-icon=\"ni ni-lock-circle-open\"\n                              placeholder=\"password\"\n                              type=\"password\"\n                              name=\"Password\"\n                              :rules=\"{required: true, min: 6}\"\n                              v-model=\"model.password\">\n                  </base-input>\n                  <div class=\"text-muted font-italic\"><small>password strength: <span\n                    class=\"text-success font-weight-700\">strong</span></small></div>\n                  <b-row class=\" my-4\">\n                    <b-col cols=\"12\">\n                      <base-input :rules=\"{ required: { allowFalse: false } }\" name=Privacy Policy>\n                        <b-form-checkbox v-model=\"model.agree\">\n                          <span class=\"text-muted\">I agree with the <a href=\"#!\">Privacy Policy</a></span>\n                        </b-form-checkbox>\n                      </base-input>\n                    </b-col>\n                  </b-row>\n                  <div class=\"text-center\">\n                    <b-button type=\"submit\" variant=\"primary\" class=\"mt-4\" @click=\"saveSystemUser\">Create account</b-button>\n                  </div>\n                </b-form>\n              </validation-observer>\n            </b-card-body>\n          </b-card>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n<script>\nimport { saveUser } from '../../api/users.api'\n  export default {\n    name: 'register',\n    data() {\n      return {\n        model: {\n          username: '',\n          user_email: '',\n          password: '',\n          agree: false\n        }\n      }\n    },\n    methods: {\n      \n     addItem() {\n      this.items.push({\n        entity:''\n      });\n    },\n    async saveSystemUser() {\n      this.error = '';\n      let response = '';\n      try{\n        this.$loading.show({delay:0});\n        response = await saveUser (\n          this.model.username, \n          this.model.user_email, \n          this.model.password,\n          )\n        console.log(response.data);\n      }catch(e){\n        this.$loading.hide();\n        let respmess = insuranceErrors.getErrorMessages(e.response.status);\n        this.error = respmess;\n        throw respmess;\n      }\n      this.$loading.hide();\n      if (response.status === 200) {\n        alert('data saved successfully');\n      }\n    }\n    }\n\n  };\n</script>\n<style></style>\n"]}]}