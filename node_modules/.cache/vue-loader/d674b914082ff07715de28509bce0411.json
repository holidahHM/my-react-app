           >\n                </base-input>\n                <span>Choice: {{ i_email }}</span>\n          </b-col>\n          <b-col lg=\"6\" sm=\"12\">\n            <base-input\n            type=\"text\"\n            label=\"User Name\"\n            placeholder=\"Your User Name will appear here\"\n            v-model=\"user_Name\"\n            >\n            </base-input>\n            <span>Choice: {{ user_Name }}</span>\n        </b-col>\n        </b-row>\n <!--3rd row -->  \n          <b-row>\n             <b-col lg=\"6\" sm=\"12\" xl=\"6\">   \n           <h4>Select Business Division(s)</h4>  \n        <b-row>\n          <b-col lg=\"3\" sm=\"3\">\n              <label class=\"form-checkbox\" for=\"pension\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"pension\" v-model=\"pensionChecked\" true-value=\"1\" false-value=\"0\">\n               Pension\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"insurance\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"insurance\" v-model=\"insuranceChecked\" true-value=\"1\" false-value=\"0\">\n               Insurance\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"trust\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"trust\" v-model=\"trustChecked\" true-value=\"1\" false-value=\"0\">\n               Trust\n              </label> \n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"acturial\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"acturial\" v-model=\"acturialChecked\" true-value=\"1\" false-value=\"0\">\n              Acturial\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"training\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"training\" v-model=\"trainingChecked\" true-value=\"1\" false-value=\"0\">\n              Training\n              </label>\n          </b-col>\n        </b-row>\n          </b-col>\n            <!--The Roles should be linked to Selected Business Development Division(s)selected-->\n            <b-col lg=\"6\" sm=\"12\">\n            <base-input\n              label=\"Role\"\n            >\n            <select class=\"form-control\" v-model=\"user_roles\">\n                <option>Business Development Officer</option>\n                <option>Business Development Executive Officer</option>\n                <option>Pension Administrator</option>\n                <option>Accountant</option>\n                <option>Fund Accountant</option>\n                <option>CRM</option>\n                <option>Senior Administrator</option>\n                <option>Head of Scheme & Administration</option>\n                <option>HR</option>\n                <option>Senior Pension Administrator</option>\n                <option>E-learning Administrator</option>\n                <option>Content Creator</option>\n                <option>Sys Admin</option>\n\n            </select>\n            <span>Choice: {{ user_roles }}</span>\n            </base-input>\n          </b-col>\n          <br>  \n          </b-row>      \n         <b-row>\n        <b-col lg=\"6\" sm=\"12\" xl=\"6\">   \n           <h4>Select Role Right(s)</h4>  \n        <b-row>\n          <b-col lg=\"3\" sm=\"3\">\n              <label class=\"form-checkbox\" for=\"admin\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"admin\" v-model=\"adminPicked\" true-value=\"1\" false-value=\"0\">\n               Admin\n              </label>\n          </b-col>\n          <b-col lg=\"4\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"approver\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"approver\" v-model=\"approverPicked\" true-value=\"1\" false-value=\"0\">\n               Approver\n              </label>\n          </b-col> \n          <b-col lg=\"4\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"supervisor\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"supervisor\" v-model=\"supervisorPicked\" true-value=\"1\" false-value=\"0\">\n               Supervisor\n              </label> \n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"member\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"member\" v-model=\"memberPicked\" true-value=\"1\" false-value=\"0\">\n              Member\n              </label>\n          </b-col>\n        </b-row>\n          </b-col>\n          <!--Access Zones -->\n          <b-col lg=\"6\" xl =\"6\" sm =\"12\">\n            <base-input\n              label=\"Zone\"\n            >\n            <select v-model=\"user_zone\" class=\"form-control\">\n              <!--1st tier-->\n                <option>Master Setup</option>\n                <option>BD</option>\n                <option>Onboarding</option>\n                <option>CRM</option>\n                <option>Servicing</option>\n                 <!--2nd Tier-->\n                <option>Accounting</option>\n                <option>Governance</option>\n                <option>Insurance</option>\n                <option>Election</option>\n                <option>Reports</option>\n                <option>Document centre</option>\n                <option>E-learning</option>\n                <option>Communication</option>\n                <option>E-board</option>\n                <option>Fund management</option>\n            </select>\n            <span>Choice: {{ user_zone }}</span>\n            </base-input>\n          </b-col>\n         </b-row>\n         <b-row>\n       \n          <b-col lg=\"6\">\n            <base-input\n              label=\"Access to\"\n            >\n           <select v-model=\"user_accessto\" class=\"form-control\">\n                <option v-if=\"user_zone ==='Master Setup'\">Master-setup - Product Setting</option>\n                <option v-if=\"user_zone ==='Master Setup'\">Master-setup - Module setting</option>\n                <option v-if=\"user_zone ==='Master Setup'\">Master-setup - User rights</option>\n                <option v-if=\"user_zone ==='Master Setup'\">Master-setup - (All Access)</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Targets and Incentives</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Prospects</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Business Lead</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Meetings</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Expense Approvals</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Onboarding</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Executive Dashboard</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Executive Targets and Incentives</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Executive Reports</option>\n                <option v-if=\"user_zone ==='BD'\"> BD - Supervisor Dashboard</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Supervisor Expense Approval</option>\n                <option v-if=\"user_zone ==='BD'\">BD - Supervisor Reports</option>\n                <option v-if=\"user_zone ==='BD'\">BD - (All Access)</option>\n                <option v-if=\"user_zone ==='Onboarding'\">Onboarding - Retail</option>\n                <option v-if=\"user_zone ==='Onboarding'\">Onboarding - Corporate</option>\n                <option v-if=\"user_zone ==='Onboarding'\">Onboarding - (All Access)</option>\n                <option v-if=\"user_zone ==='Servicing'\">Servicing - Schemes</option>\n                <option v-if=\"user_zone ==='Servicing'\">Servicing - Member Servicing</option>\n                <option v-if=\"user_zone ==='Servicing'\">Servicing - Contributions</option>\n                <option v-if=\"user_zone ==='Servicing'\">Servicing - Claims</option>\n                <option v-if=\"user_zone ==='Servicing'\">Servicing - Benefits</option>\n                <option v-if=\"user_zone ==='Servicing'\">Servicing - (All Access)</option>\n            </select>\n            <span>Choice: {{ user_accessto }}</span>\n            </base-input>\n          </b-col>\n          <b-col lg=\"6\">\n            <base-input\n              label=\"User Type\"\n            >\n           <select class=\"form-control\" v-model=\"user_type\">\n                <option>Internal</option>\n            </select>\n            <span>Choice: {{ user_type }}</span>\n            </base-input>\n          </b-col>\n        </b-row>\n        <b-row>\n               <b-col lg=\"12\" sm=\"12\" xl=\"12\">\n                 <h4>Select Scheme</h4>\n           <div style=\"overflow: scroll;\"> \n\t  <b-row>\n\t  <b-col xl=\"12\" lg=\"12\" sm=12>\n    <span>Choice: {{ selected }}</span>\n<table class=\"table table-stripped\">\n\t    <thead class=\"thead-light\">\n\t      <tr>\n\t\t<th scope=\"col\">\n\t\t\t<label class=\"form-checkbox\">\n\t\t\t<input type='checkbox' v-model='selectAll' @click='select'> \n\t\t\t</label>\n\t\t\t</th>\n\t      <th scope=\"col\">Scheme Id</th>\n\t      <th scope=\"col\">Scheme Code</th >\n\t\t\t <th scope=\"col\">Scheme Name</th>\n\t\t\t <th scope=\"col\">Country</th>\n       <th scope=\"col\">Action</th>\n\n\t      </tr>\n\t    </thead>\n       <tbody  id=\"myTable\">\n\t      \t      \t<tr v-for=\"i in items\">\n\t\t\t\t<td>\n\t\t\t\t\t\t<label class=\"form-checkbox\" for=\"schemes\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"schemes\" :value=\"i.scheme_code \" v-model=\"selected\">\n\t\t\t\t\t\t<i class=\"form-icon\"></i>\n  \t\t\t\t\t</label>\n\t\t\t\t</td>\n\t      \t\t<td>{{i.scheme_id}}</td>\n\t      \t\t<td>{{i.scheme_code}}</td>\n\t      \t\t<td>{{i.scheme_name}}</td>\n\t      \t\t<td>{{i.scheme_country}}</td>\n\n<td>\n\t<b-dropdown id=\"dropdown-left\" text=\"Action\" variant=\"primary\" class=\"m-2\">\n    <b-dropdown-item href=\"#\">Open</b-dropdown-item>\n    <b-dropdown-item href=\"#\">Edit</b-dropdown-item>\n    <b-dropdown-item href=\"#\">Delete</b-dropdown-item>\n\t</b-dropdown>\n\t\t\t\t\t</td>\n\t      \t</tr>\n\t\t\t   </tbody>\n\t  </table>\n\t  </b-col>\n\t  </b-row>\n  </div>\n          </b-col>\n          </b-row>     \n      </div>\n      <br>\n      <br>\n      <b-row class=\"d-flex justify-content-center mb-3\"  >\n        \n            <base-button type=\"info\" @click.prevent=\"addinternalUser\">Save</base-button>\n        \n            <base-button type=\"danger\">Delete</base-button>\n        \n        </b-row>\n        <hr class=\"my-4\">\n\n        <div class=\"alert alert-danger\" v-if=\"error\">\n            {{ error }}\n          </div>\n          <div class=\"alert alert-success\" v-if=\"successData\">\n            {{ successData }}\n          </div>\n\n    </b-form>\n      \n    </b-tab>\n    <!--External Members Pill -->\n     <b-tab>\n      <template v-slot:title >\n        External User\n      </template>\n      <br>\n      <b-form @submit.prevent=\"updateProfile\">\n        </b-form>\n        <h3 class=\"mb-0\">Add /Edit External User details</h3>\n      <br>\n      <div class=\"pl-lg-4\">\n        <b-row>\n            <b-col lg=\"6\">\n                <base-input\n                type=\"text\"\n                label=\"First name\"\n                placeholder=\"Enter your First Name\"\n                v-model=\"user_f_name\"\n                >\n                </base-input>\n          </b-col>\n         <b-col lg=\"6\">\n                <base-input\n                type=\"text\"\n                label=\"Last name\"\n                placeholder=\"Enter your Last Name\"\n                v-model=\"user_l_name\"\n                >\n                </base-input>\n          </b-col>\n        </b-row>\n        <b-row>\n               <b-col lg=\"6\">\n            <base-input\n              label=\"Choose Division\"\n             >\n            <select class=\"form-control\" v-model=\"user_division\">\n                <option>Pension</option>\n                <option>Insurance</option>\n                <option>Trust</option>\n                <option>Training</option>\n                <option>Acturial Services</option>\n            </select>\n            <span>Choice: {{ user_division }}</span>\n            </base-input>\n          </b-col>\n          <b-col lg=\"6\">\n                <base-input\n                type=\"email\"\n                label=\"User Email\"\n                placeholder=\"Enter your Email Address\"\n                v-model=\"user_email\"\n                >\n                </base-input>\n          </b-col>\n        </b-row>\n         <b-row>\n          <b-col lg=\"6\">\n            <base-input\n              label=\"Role\"\n            >\n            <select class=\"form-control\" v-model=\"user_division_role\">\n                <option :value=\"role.role_id\" v-for=\"role in e_roles\" v-bind:key=\"role.role_id\">\n                  {{ role.role_name }}\n                </option>\n            </select>\n            </base-input>\n          </b-col>\n      <b-col lg=\"6\">\n            <base-input\n              label=\"Sub Role\"\n            >\n            <select class=\"form-control\" v-model=\"user_division_rights\">\n                <option>Admin</option>\n                <option>Approver</option>\n                 <option>Supervisor</option>\n                   <option>Sys Admin</option>\n            </select>\n            </base-input>\n          </b-col>\n         </b-row>\n         <b-row>\n                   <b-col lg=\"6\">\n            <base-input\n              label=\"Zone\"\n            >\n            <select class=\"form-control\" v-model=\"user_division_zone\">\n                <option>Onboarding </option>\n                <option>Accounting</option>\n                <option>Servicing</option>\n                <option>Governance</option>\n                <option>Insurance</option>\n                 <option>Election</option>\n                  <option>BD</option>\n                   <option>Governance</option>\n                    <option>E-learning</option>\n                     <option>Fund Management</option>\n            </select>\n            </base-input>\n          </b-col>\n          <b-col lg=\"6\">\n            <base-input\n              label=\"Access to\"\n            >\n           <select class=\"form-control\" v-model=\"user_division_accessto\">\n                <option>Onboarding</option>\n                <option>CRM</option>\n                <option>BD</option>\n                <option>Accounting</option>\n                <option>Servicing</option>\n                <option>Governance</option>\n                <option>Insurance</option>\n                 <option>Election</option>\n                  <option>BD</option>\n                   <option>Onboarding</option>\n                    <option>E-learning</option>\n            </select>\n            </base-input>\n          </b-col>\n        </b-row>\n        <b-row>\n               <b-col lg=\"6\">\n            <base-input\n              label=\"User Type\"\n            >\n           <select class=\"form-control\" v-model=\"user_type_div\">\n                <option>External</option>\n            </select>\n            <span>Choice: {{ user_type_div }}</span>\n            </base-input>\n          </b-col>\n          </b-row>\n      \n      </div>\n      <br>\n      <br>\n      <b-row class=\"d-flex justify-content-center mb-3\"  >\n        \n            <base-button type=\"info\" @click.prevent=\"addExternalUser\">Save</base-button>\n        \n            <base-button type=\"danger\">Delete</base-button>\n        \n        </b-row>\n        <hr class=\"my-4\">\n        <div class=\"alert alert-danger\" v-if=\"errorExternal\">\n            {{ errorExternal }}\n          </div>\n          <div class=\"alert alert-success\" v-if=\"successDataExternal\">\n            {{ successDataExternal }}\n          </div>\n\n      </b-tab> \n  </b-tabs>\n</div>\n  </card>\n</base-header>\n</template>\n<script>\nimport { getAllSchemes, getExternalRoles, saveExternalUser, saveInternalUser } from '../../../api/users.api';\nimport BaseInput from '../../../components/Inputs/BaseInput.vue';\nimport insuranceErrors from '../../../service/insuranceErrors';\nexport default {\n  components: { BaseInput },\n  data() {\n    return {\n      i_f_name:'', //i stands for internal\n      i_l_name:'',\n      i_email:'',\n      user:'',\n      user_f_name: '',\n      user_l_name: '',\n      user_email: '',\n      adminPicked: '',\n      approverPicked: '',\n      supervisorPicked: '',\n      memberPicked: '',\n      pensionChecked: '',\n      insuranceChecked: '',\n      trustChecked: '',\n      error: '',\n      errorExternal: '',\n      successDataExternal: '',\n      user_type: '',\n      successData: '',\n      acturialChecked: '',\n      trainingChecked: '',\n      user_zone:'',\n      user_roles: '',\n      user_accessto:'',\n      b_division:['Default'], // Array reference\n        lists: [\n          { value: 'Default', text: \"You haven't choosen any value\", disabled: true  },\n          { value: 'Pension', text: 'Pension' },\n          { value: 'Insurance', text: 'Insurance' },\n          { value: 'Trust', text: 'Trust' },\n          { value: 'Acturial', text: 'Acturial'},\n          { value: 'Training', text: 'Training'},\n        ],\n      items: [],\n\t\tselected: [],\n\t\tselectAll: false,\n    e_roles: [],\n      }\n  },\n  computed:{\n    user_Name:function(){\n      return this.i_f_name +('.')+ this.i_l_name;\n    },\n  },\n  methods: {\n    updateProfile() {\n      alert('Your data: ' + JSON.stringify(this.user));\n    },\n     select() {\n\t\t\tthis.selected = [];\n\t\t\tif (!this.selectAll) {\n\t\t\t\tfor (let i in this.items) {\n\t\t\t\t\tthis.selected.push(this.items[i].scheme_code );\n\t\t\t\t}\n\t\t\t}\n\t\t},\n    async addinternalUser() {\n      let response = '';\n      this.error = '';\n      this.successData = '';\n      try {\n        this.$loading.show({delay:0});\n        response = await saveInternalUser(\n          this.user_Name,\n          this.i_email,\n          this.i_f_name,\n          this.i_l_name,\n          this.user_roles,\n          this.user_type,\n          this.user_accessto,\n          this.selected,\n          this.pensionChecked,\n          this.insuranceChecked,\n          this.trustChecked,\n          this.acturialChecked,\n          this.trainingChecked,\n          this.adminPicked,\n          this.approverPicked,\n          this.supervisorPicked,\n          this.memberPicked\n        );\n        console.log(response);\n      } catch (e) {\n        this.$loading.hide();\n        let resp = '';\n        resp = insuranceErrors.getErrorInternalUser(e.response.status);\n        this.error = resp;\n        throw resp;\n      }\n      this.$loading.hide();\n      if (response.status === 200) {\n        //alert('data saved successfully');\n        this.successData = 'user saved successfully';\n      }\n    },\n    async addExternalUser() {\n      this.errorExternal = '';\n      this.successDataExternal = '';\n      let response = '';\n      try {\n        this.$loading.show({delay:0});\n        response = await saveExternalUser();\n        console.log(response);\n      } catch (e) {\n        this.$loading.hide();\n        let resp = '';\n        resp = insuranceErrors.getErrorExternalUser(e.response.status);\n        this.errorExternal = resp;\n        throw resp;\n      }\n      this.$loading.hide();\n      if (response.status === 200) {\n        //alert('data saved successfully');\n        this.successDataExternal = 'user saved successfully';\n      }\n    },\n  },\n  async mounted() {\n    if (localStorage.getItem('allSchemes') || localStorage.getItem('externalRoles')) {\n      this.items = JSON.parse(localStorage.getItem('allSchemes'));\n      this.e_roles = JSON.parse(localStorage.getItem('externalRoles'));\n    }\n    try {\n      let response = await getAllSchemes();\n      this.items = response.data.data;\n      console.log(response.data);\n      localStorage.setItem('allSchemes', JSON.stringify(response.data.data));\n    } catch (e) {\n      \n    }\n    try {\n      let response = await getExternalRoles();\n      this.e_roles = response.data.data;\n      console.log(response.data);\n      localStorage.setItem('externalRoles', JSON.stringify(response.data.data));\n    } catch (e) {\n      \n    }\n  },\n  \n};\n</script>\n<style></style>\n"]}]}