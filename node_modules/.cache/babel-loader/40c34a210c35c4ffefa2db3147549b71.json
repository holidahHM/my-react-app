ws.push({});\n  //     },\n  //     removeRow: function(row){\n\n  //       this.rows.$remove(row);\n  //     }\n  //   }\n  data() {\n    users: [{ name: \"\", email: \"\", mobno: \"\" }];\n  },\n  methods: {\n    addRow: function () {\n      this.users.push({ name: \"\", email: \"\", mobno: \"\" });\n    },\n    deleteRow(index) {\n      this.users.splice(index, 1);\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Sites/onboarding/Retail/Insurance/Easy Cover"}]}