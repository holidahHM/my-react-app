A;;AAAA;AAAA;AAAA;AApDA,CAAA","sourcesContent":["<template>\n  <base-header class=\"pb-6 pb-8 pt-5 pt-md-8 bg-gradient-success\">\n    <card>\n      <form-wizard>\n        <tab-content title=\"Personal Details\" :selected=\"true\">\n          <div class=\"tab\">\n            <div align-v=\"center\" slot=\"header\">\n              <b-col cols=\"12\" class=\"d-flex justify-content-center mb-3\">\n                <strong>Personal Details(Principal Member)</strong>\n              </b-col>\n            </div>\n            <div class=\"row\">\n              <div class=\"col-sm-4\">\n                <div class=\"form-group\">\n                  <label class=\"control-label\">Full Names *</label>\n\n                  <input\n                    type=\"text\"\n                    name=\"member_name\"\n                    required\n                    id=\"m_name\"\n                    class=\"form-control\"\n                  />\n                </div>\n              </div>\n\n              <input type=\"hidden\" name=\"member_id\" class=\"form-control\" />\n              <div class=\"col-sm-4\">\n                <div class=\"form-group\">\n                  <label class=\"control-label\">Email * </label>\n                  <input\n                    type=\"text\"\n                    name=\"member_email\"\n                    id=\"m_email\"\n                    class=\"form-control\"\n                    required\n                  />\n                </div>\n              </div>\n\n              <!-- <div class=\"col-sm-3\">\n                        \n                                <div class=\"form-group\">\n                                    <label class=\"control-label\">Place of Work/Business</label>\n                                \n                                    <input type=\"text\"  name=\"member_workplace\"  id=\"m_payroll_number\"   class=\"form-control\"  />\n                                </div>\n                            </div> -->\n              <div class=\"col-sm-4\">\n                <div class=\"form-group\">\n                  <label class=\"control-label\">Phone Number *</label>\n                  <input\n                    type=\"text\"\n                    name=\"member_tel_no\"\n                    id=\"phone_number\"\n                    class=\"form-control\"\n                    required\n                  />\n                </div>\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-sm-6\">\n                <div class=\"form-group\">\n                  <label class=\"control-label\">ID Number *</label>\n                  <input\n                    type=\"text\"\n                    name=\"member_id_no\"\n                    id=\"m_id_number\"\n                    class=\"form-control\"\n                    required\n                  />\n                </div>\n              </div>\n\n              <div class=\"col-sm-6\">\n                <div class=\"form-group\">\n                  <label class=\"control-label\">Date of Birth *</label>\n                  <input\n                    type=\"date\"\n                    name=\"member_dob\"\n                    id=\"from-datepicker\"\n                    placeholder=\"YYYY-MM-DD\"\n                    class=\"form-control inputdate\"\n                    required\n                  />\n                </div>\n              </div>\n\n              <!-- <div class=\"col-sm-3\">\n                                <div class=\"form-group\">\n                                    <label class=\"control-label\">KRA PIN</label>\n                                    <input type=\"text\" name=\"member_kra_pin\" id=\"m_pin\" class=\"form-control\"  >\n                                 </div>\n                                \n                            </div>-->\n            </div>\n          </div>\n        </tab-content>\n        <tab-content title=\"Dependants\">\n          <div class=\"tab\">\n            <div class=\"row\">\n              <h3><label class=\"control-label\">Who Do You want to Cover</label><br /></h3>\n\n              <div class=\"col-sm-9\">\n                <h5 class=\"lg-title\">Dependants</h5>\n                <p class=\"mb20\">\n                  Fill out the table below to register your dependants. Use the \"+\" and\n                  \"-\" button to add and remove dependants respectively.\n                </p>\n              </div>\n              <div class=\"col-sm-3\">\n                <a\n                  @click.prevent=\"addNewDependant\"\n                  id=\"add_row\"\n                  class=\"btn btn-info pull-right\"\n                  ><i class=\"fa fa-plus\"></i\n                ></a>\n              </div>\n            </div>\n            <div style=\"overflow: scroll\">\n              <table class=\"table table-bordered table-hover\" id=\"tab_logic\">\n                <thead>\n                  <tr>\n                    <th class=\"text-center\" style=\"width: 150px\">Remove</th>\n                    <th class=\"text-center\" style=\"width: 150px\">Relationship</th>\n                    <th class=\"text-center\" style=\"width: 200px\">Dependant Name *</th>\n                    <th class=\"text-center\" style=\"width: 100px\">Date of Birth *</th>\n                    <th class=\"text-center\" style=\"width: 100px\">Identifying Document</th>\n                    <th class=\"text-center\" style=\"width: 50px\">ID Document Number</th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    id=\"addr0\"\n                    v-for=\"(depend, index) in dependant_List\"\n                    :key=\"depend.id\"\n                  >\n                    <td>\n                      <a\n                        v-on:click=\"dependant_List.splice(index, 1)\"\n                        id=\"delete_row\"\n                        class=\"pull-right btn btn-danger mr20 mb10\"\n                        ><i class=\"fa fa-minus\"></i\n                      ></a>\n                    </td>\n                    <td>\n                      <select\n                        v-model=\"depend.relationship\"\n                        class=\"form-control\"\n                        id=\"relationship0\"\n                        name=\"insuree_id\"\n                        required\n                      >\n                        <!-- filled using js -->\n                        <Option>father</Option>\n                        <Option>mother</Option>\n                        <Option>daughter</Option>\n                        <Option>son</Option>\n                        <Option>wife</Option>\n                        <Option>brother</Option>\n                        <Option>sister</Option>\n                        <Option>relative</Option>\n                      </select>\n                    </td>\n                    <td>\n                      <input\n                        type=\"text\"\n                        name=\"dependant_name\"\n                        required\n                        placeholder=\"Enter full name\"\n                        class=\"form-control\"\n                        v-model=\"depend.dependant_name\"\n                      />\n                    </td>\n                    <td>\n                      <input\n                        type=\"date\"\n                        name=\"dependant_dob\"\n                        required\n                        placeholder=\"yyyy-mm-dd\"\n                        class=\"form-control inputdate\"\n                        v-model=\"depend.dependant_dob\"\n                      />\n                    </td>\n                    <td>\n                      <select\n                        v-model=\"depend.dependant_id_type\"\n                        class=\"form-control\"\n                        name=\"dependant_id_doc_type[]\"\n                      >\n                        <option value=\"National ID\">National ID</option>\n                        <option value=\"Passport\">Passport</option>\n                        <option value=\"BirthCert\">Birth Certificate</option>\n                      </select>\n                    </td>\n                    <td>\n                      <input\n                        type=\"number\"\n                        name=\"dependant_identity_number\"\n                        placeholder=\"passport/id number\"\n                        class=\"form-control\"\n                        v-model=\"depend.id_number\"\n                      />\n                    </td>\n                  </tr>\n\n                  <tr id=\"addr1\"></tr>\n                </tbody>\n              </table>\n            </div>\n            {{ dependant_List }}\n          </div>\n        </tab-content>\n        <tab-content title=\"Beneficiaries\">\n          <div class=\"tab\">\n            <div class=\"row\" style=\"margin-top: 30px\">\n              <div class=\"col-sm-12\">\n                <h3>\n                  <label class=\"control-label\">Beneficiaries/Next of Kin</label><br />\n                </h3>\n              </div>\n              <div class=\"col-sm-9\">\n                <p class=\"mb20\">\n                  Fill out the table below to register the member's beneficiaries/next of\n                  kin. Use the \"+\" and \"-\" button to add and remove beneficiaries\n                  respectively.\n                </p>\n              </div>\n              <div class=\"col-sm-3\">\n                <a\n                  @click.prevent=\"addNewBeneficiary\"\n                  id=\"add_nok\"\n                  class=\"btn btn-info pull-right\"\n                  ><i class=\"fa fa-plus\"></i\n                ></a>\n              </div>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-md-12\">\n                <table class=\"table table-bordered table-hover\" id=\"ben_logic\">\n                  <thead>\n                    <tr>\n                      <th class=\"text-center\" style=\"width: 250px\">Remove</th>\n                      <th class=\"text-center\" style=\"width: 250px\">Beneficiary Name</th>\n                      <th class=\"text-center\" style=\"width: 200px\">\n                        Relationship to Member\n                      </th>\n                      <th class=\"text-center\" style=\"width: 100px\">Telephone Number</th>\n                    </tr>\n                  </thead>\n                  <tbody>\n                    <tr\n                      id=\"ben0\"\n                      v-for=\"(benefit, index) in beneficiary_list\"\n                      :key=\"benefit.id\"\n                    >\n                      <td>\n                        <a\n                          v-on:click=\"beneficiary_list.splice(index, 1)\"\n                          id=\"delete_row\"\n                          class=\"pull-right btn btn-danger mr20 mb10\"\n                          ><i class=\"fa fa-minus\"></i\n                        ></a>\n                      </td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          name=\"beneficiary_name[]\"\n                          placeholder=\"Enter full name\"\n                          class=\"form-control\"\n                          v-model=\"benefit.beneficiary_name\"\n                        />\n                      </td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          name=\"beneficiary_relationship[]\"\n                          class=\"form-control\"\n                          v-model=\"benefit.beneficiary_relationship\"\n                        />\n                      </td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          name=\"beneficiary_tel_no[]\"\n                          class=\"form-control\"\n                          v-model=\"benefit.beneficiary_phone\"\n                        />\n                      </td>\n                    </tr>\n                    <tr id=\"ben1\"></tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n          </div>\n        </tab-content>\n        <tab-content title=\"Step 3\">\n          <div class=\"tab\">\n            Payments And Declaration:\n            <div class=\"row\">\n              <div class=\"col-sm-9\">\n                <h3 class=\"lg-title\">Policy Payment Details</h3>\n\n                <h4>Your policy summery</h4>\n\n                <p class=\"mb20\"></p>\n                <h4>Payment Procedure</h4>\n\n                <ul>\n                  <li>SELECT M-PESA from the Menu.</li>\n                  <li>\n                    Lipa na M-PESA > Buy Goods And services 'till no' =\n                    <strong>920829</strong>\n                  </li>\n                  <li>\n                    Enter Amount > Enter amount as shown in the field below \"Amount To be\n                    Paid\"\n                  </li>\n                  <li>Enter Your Mpesa pin</li>\n                </ul>\n              </div>\n              <p class=\"mb20\">Fill out the payment details .</p>\n            </div>\n\n            <div class=\"row\">\n              <div class=\"col-md-12\">\n                <table class=\"table table-bordered table-hover\" id=\"pay_logic\">\n                  <thead></thead>\n                  <tbody>\n                    <tr>\n                      <td><strong>Amount To be Paid</strong></td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          readonly\n                          name=\"payment_amount[]\"\n                          id=\"payment_due0\"\n                          class=\"form-control\"\n                          v-model=\"paymentAmount\"\n                        />\n                      </td>\n                    </tr>\n                    <tr>\n                      <td><strong> Mode of Payment </strong></td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          readonly\n                          name=\"mode_of_payment[]\"\n                          value=\"MPESA\"\n                          id=\"mode_of_payment\"\n                          class=\"form-control\"\n                          v-model=\"mode_of_payment\"\n                        />\n                      </td>\n                    </tr>\n\n                    <tr>\n                      <td><strong> Mpesa phone number(+254) *</strong></td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          name=\"mpesa_phone_number\"\n                          id=\"mpesa_phone_number\"\n                          required\n                          class=\"form-control\"\n                          v-model=\"phone_number\"\n                        />\n                      </td>\n                    </tr>\n\n                    <tr>\n                      <td><strong> Mpesa Code *</strong></td>\n                      <td>\n                        <input\n                          type=\"text\"\n                          name=\"payment_ref[]\"\n                          id=\"payment_ref\"\n                          required\n                          class=\"form-control\"\n                          v-model=\"mpesa_code\"\n                        />\n                      </td>\n                    </tr>\n\n                    <tr></tr>\n                    <tr id=\"pay1\"></tr>\n                  </tbody>\n                </table>\n              </div>\n            </div>\n            <br />\n            <button\n              style=\"margin-bottom: 10%; float: right\"\n              class=\"btn btn-success\"\n              @click.prevent=\"confirmPayment\"\n            >\n              Confirm Payment\n            </button>\n            <button style=\"margin-bottom: 10%; float: right\" class=\"btn btn-info\">\n              Pay Later\n            </button>\n            <br />\n            <br />\n            <br />\n            <div class=\"alert alert-danger\" v-if=\"errormsg\">\n              {{ errormsg }}\n            </div>\n          </div>\n        </tab-content>\n      </form-wizard>\n    </card>\n  </base-header>\n</template>\n\n<script>\nimport { FormWizard, TabContent } from \"vue-step-wizard\";\nimport \"vue-step-wizard/dist/vue-step-wizard.css\";\nexport default {\n  name: \"BasicStepperForm\",\n  components: {\n    FormWizard,\n    TabContent,\n  },\n  data() {\n    return {\n      email: \"\",\n      companyName: \"\",\n      referral: \"\",\n      errormsg: \"\",\n      mpesa_code: \"\",\n      phone_number: \"\",\n      paymentAmount: \"5,000\",\n      mode_of_payment: \"MPESA\",\n      //first step data;\n      relationship: \"\",\n      dependant_name: \"\",\n      dependant_dob: \"\",\n      dependant_id_type: \"\",\n      id_number: \"\",\n      dependant_List: [\n        {\n          id: 1,\n          relationship: \"\",\n          dependant_name: \"\",\n          dependant_dob: \"\",\n          dependant_id_type: \"\",\n          id_number: \"\",\n        },\n      ],\n      nextId: 2,\n\n      //second step data\n      beneficiary_name: \"\",\n      beneficiary_relationship: \"\",\n      beneficiary_phone: \"\",\n      beneficiary_list: [\n        {\n          id: 1,\n          beneficiary_name: \"\",\n          beneficiary_relationship: \"\",\n          beneficiary_phone: \"\",\n        },\n      ],\n      nextBn: 2,\n    };\n  },\n  created() {\n    this.dependant_List;\n  },\n  methods: {\n    addNewDependant() {\n      this.dependant_List.push({\n        id: this.nextId++,\n        relationship: this.relationship,\n        dependant_name: this.dependant_name,\n        dependant_dob: this.dependant_dob,\n        dependant_id_type: this.dependant_id_type,\n        id_number: this.id_number,\n      });\n      this.relationship = \"\";\n      this.dependant_name = \"\";\n      this.dependant_dob = \"\";\n      this.dependant_id_type = \"\";\n      this.id_number = \"\";\n    },\n    removeDependant() {\n      for (let i in this.dependant_List) {\n        this.dependant_List.splice(i, 1);\n      }\n    },\n    addNewBeneficiary() {\n      this.beneficiary_list.push({\n        id: this.nextBn++,\n        beneficiary_name: this.beneficiary_name,\n        beneficiary_relationship: this.beneficiary_relationship,\n        beneficiary_phone: this.beneficiary_phone,\n      });\n      this.beneficiary_name = \"\";\n      this.beneficiary_relationship = \"\";\n      this.beneficiary_phone = \"\";\n    },\n    removeBeneficiary() {\n      for (let i in this.beneficiary_list) {\n        this.beneficiary_list.splice(i, 1);\n      }\n    },\n    async confirmPayment() {\n      this.$toast.error(\"Failed! No mpesa code detected\", {\n        position: \"top-right\",\n        timeout: 5000,\n        closeOnClick: false,\n        pauseOnFocusLoss: true,\n        pauseOnHover: true,\n        draggable: true,\n        draggablePercent: 0.6,\n        showCloseButtonOnHover: true,\n        hideProgressBar: false,\n        closeButton: \"button\",\n        icon: true,\n        rtl: false,\n      });\n    },\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Sites/onboarding/Retail/Insurance/Easy Cover"}]}