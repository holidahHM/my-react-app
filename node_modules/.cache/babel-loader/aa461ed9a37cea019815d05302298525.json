    >Generate Statement\n              </router-link></b-dropdown-item\n            >\n          </b-dropdown>\n        </b-button-group>\n      </b-col>\n    </b-row> -->\n    <b-row>\n      <b-col xl=\"12\" md=\"12\">\n        <card>\n          <b-tabs card vertical>\n            <b-tab\n              xl=\"3\"\n              :title=\"count.Products\"\n              active\n              v-for=\"count in memberDet\"\n              v-bind:key=\"count.ProposalNo\"\n            >\n              <b-row>\n                <b-col class=\"col-md-12\">\n                  <div class=\"table-responsive\">\n                    <table class=\"table table-bordered mb30\">\n                      <thead></thead>\n\n                      <tbody>\n                        <tr>\n                          <td><strong>Member Name </strong></td>\n                          <td>{{ i_f_name }}</td>\n                        </tr>\n                        <tr>\n                          <td><strong>Product Name</strong></td>\n                          <td>{{ count.Products }}</td>\n                        </tr>\n                        <tr>\n                          <td><strong>Policy Number </strong></td>\n                          <td>\n                            {{ count.Policyno }}\n                          </td>\n                        </tr>\n                        <tr>\n                          <td><strong>Insurance Company</strong></td>\n                          <td>\n                            {{ count.Company }}\n                          </td>\n                        </tr>\n                        <tr>\n                          <td><strong>Amount Insured</strong></td>\n                          <td>\n                            {{ count.SumInsured }}\n                          </td>\n                        </tr>\n                        <tr>\n                          <td><strong>Option Category</strong></td>\n                          <td>{{ count.Option_Category }}</td>\n                        </tr>\n                        <!-- <tr>\n                          <td>\n                            <strong>Policy Certificate (password protected)</strong>\n                          </td>\n                          <td>\n                            <a class=\"btn btn-primary\" target=\"_blank\" href=\"\">View </a>\n                          </td>\n                        </tr> -->\n\n                        <tr>\n                          <td><strong>Policy Status</strong></td>\n\n                          <td>\n                            <span class=\"badge badge-primary\">{{ count.Status }}</span>\n                          </td>\n                        </tr>\n                        <tr>\n                          <td><strong>Policy Start</strong></td>\n                          <td>\n                            {{ count.From.date }}\n                          </td>\n                        </tr>\n\n                        <tr>\n                          <td><strong>Policy End</strong></td>\n                          <td>\n                            {{ count.To.date }}\n                          </td>\n                        </tr>\n                      </tbody>\n                    </table>\n                    <br />\n                    <b-row class=\"d-flex justify-content-center mb-3\">\n                      <base-button type=\"success\">Renew</base-button>\n                      <base-button type=\"info\">Suspend</base-button>\n                      <base-button type=\"danger\">Delete</base-button>\n                    </b-row>\n                  </div>\n                </b-col>\n              </b-row>\n            </b-tab>\n          </b-tabs>\n        </card>\n      </b-col>\n    </b-row>\n  </base-header>\n</template>\n<script>\nimport { getSingleMemberPolicy } from \"../../../../api/users.api\";\nimport Card from \"../../../../components/Cards/Card.vue\";\nexport default {\n  components: { Card },\n  data() {\n    return {\n      memberDet: [],\n      member_ID: \"\",\n      i_f_name: \"\",\n      prod_num: \"\",\n    };\n  },\n  created() {\n    this.member_ID = this.$route.params.member_id;\n  },\n  async mounted() {\n    let response = \"\";\n    try {\n      this.$loading.show({ delay: 0 });\n      response = await getSingleMemberPolicy(this.member_ID);\n      console.info(\"member\", response.data.data);\n      this.memberDet = response.data.data;\n    } catch (e) {\n      this.$loading.hide();\n      console.error(\"error\", e);\n      this.$toast.error(\"Failed! Refresh the page\", {\n        position: \"top-right\",\n        timeout: 5000,\n        closeOnClick: true,\n        pauseOnFocusLoss: true,\n        pauseOnHover: true,\n        draggable: true,\n        draggablePercent: 0.6,\n        showCloseButtonOnHover: true,\n        hideProgressBar: false,\n        closeButton: \"button\",\n        icon: true,\n        rtl: false,\n      });\n    }\n    this.$loading.hide();\n\n    if (response.status === 200) {\n      this.$toast.success(\"Member retrieved\", {\n        position: \"top-right\",\n        timeout: 3000,\n        closeOnClick: true,\n        pauseOnFocusLoss: true,\n        pauseOnHover: true,\n        draggable: true,\n        draggablePercent: 0.6,\n        showCloseButtonOnHover: true,\n        hideProgressBar: false,\n        closeButton: \"button\",\n        icon: true,\n        rtl: false,\n      });\n      for (let item in this.memberDet) {\n        this.i_f_name = this.memberDet[item].Insured;\n      }\n    }\n\n    //this.i_f_name = this.memberDet.Name;\n  },\n\n  methods: {},\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views/Sites/Servicing/Members"}]}