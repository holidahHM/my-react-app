eAAA,EAlJA;;AAAA;AAkJA,cAAA,IAlJA;AAmJA,mBAAA,YAAA,GAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA;AACA,cAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AArJA;AAAA;;AAAA;AAAA;AAAA;AAuJA,cAAA,OAAA,CAAA,GAAA;;AAvJA;AAAA;AAAA;AAAA,qBA0JA,gBAAA,EA1JA;;AAAA;AA0JA,cAAA,SA1JA;AA2JA,mBAAA,OAAA,GAAA,SAAA,CAAA,IAAA,CAAA,IAAA;AACA,cAAA,OAAA,CAAA,GAAA,CAAA,SAAA,CAAA,IAAA;AACA,cAAA,YAAA,CAAA,OAAA,CAAA,eAAA,EAAA,IAAA,CAAA,SAAA,CAAA,SAAA,CAAA,IAAA,CAAA,IAAA,CAAA;AA7JA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAkKA,EAAA,OAlKA,qBAkKA;AACA,IAAA,OAAA,CAAA,GAAA,CAAA,cAAA,EAAA,KAAA,kBAAA;AACA;AApKA,CAAA","sourcesContent":["<template>\n<base-header class=\"pb-6 pb-8 pt-5 pt-md-8 bg-gradient-success\">\n    <card>\n      \t<b-row class=\"float-right pb-4\">\n\t\t  <b-col xl=\"12\" >\n\t\t\t  <router-link to=\"/view_members\">\n\t\t\t\t<b-button variant=\"primary\">Back</b-button>\n\t\t\t  </router-link>\n\t\t  </b-col>\n\t  </b-row>\n    <div>\n  <b-tabs pills>\n    <!--Internal Member Nav Pill -->\n    <b-tab active>\n      <template v-slot:title >\n      Internal User\n      </template>\n      <br>\n  <b-form @submit.prevent=\"updateProfile\">\n  <h3 class=\"mb-4\">Add / Edit Internal User details</h3>\n      <!--Div for the whole internal member section -->\n      <div class=\"pl-lg-4\"> \n<!--1st row -->\n        <b-row>\n          <br>\n            <b-col lg=\"6\" sm=\"12\">\n                <base-input\n                type=\"text\"\n                label=\"First Name\"\n                placeholder=\"Enter your First Name\"\n                v-model=\"i_f_name\"\n                >\n                </base-input>\n                <span>Choice: {{ i_f_name }}</span>\n          </b-col>\n         <b-col lg=\"6\" sm=\"12\">\n                <base-input\n                type=\"text\"\n                label=\"Last Name\"\n                placeholder=\"Enter your Last Name\"\n                v-model=\"i_l_name\"\n                >\n                </base-input>\n                <span>Choice: {{ i_l_name }}</span>\n          </b-col>\n           <span>Choice: {{ user_fullNames }}</span>\n        </b-row>\n<!--2nd row -->\n      <b-row>\n          <b-col lg=\"6\" sm=\"12\">\n                <base-input\n                type=\"email\"\n                label=\"User Email\"\n                placeholder=\"Enter your Email Address\"\n                v-model=\"i_email\"\n                >\n                </base-input>\n                <span>Choice: {{ i_email }}</span>\n          </b-col>\n          <b-col lg=\"6\" sm=\"12\">\n            <base-input\n            type=\"text\"\n            label=\"User Name\"\n            placeholder=\"Your User Name will appear here\"\n            v-model=\"user_Name\"\n            >\n            </base-input>\n            <span>Choice: {{ user_Name }}</span>\n        </b-col>\n        </b-row>\n <!--3rd row -->  \n          <b-row>\n             <b-col lg=\"6\" sm=\"12\" xl=\"6\">   \n           <h4>Select Business Division(s)</h4>  \n        <b-row>\n          <b-col lg=\"3\" sm=\"3\">\n              <label class=\"form-checkbox\" for=\"pension\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"pension\" v-model=\"pensionChecked\" true-value=\"1\" false-value=\"0\">\n               Pension\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"insurance\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"insurance\" v-model=\"insuranceChecked\" true-value=\"1\" false-value=\"0\">\n               Insurance\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"trust\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"trust\" v-model=\"trustChecked\" true-value=\"1\" false-value=\"0\">\n               Trust\n              </label> \n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"acturial\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"acturial\" v-model=\"acturialChecked\" true-value=\"1\" false-value=\"0\">\n              Acturial\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"training\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"training\" v-model=\"trainingChecked\" true-value=\"1\" false-value=\"0\">\n              Training\n              </label>\n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"training\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"active\" v-model=\"user_activeChecked\" true-value=\"1\" false-value=\"0\">\n              User Active\n              </label>\n          </b-col>\n        </b-row>\n          </b-col>\n            <!--The Roles should be linked to Selected Business Development Division(s)selected-->\n            <b-col lg=\"6\" sm=\"12\">\n            <base-input\n              label=\"Role\"\n            >\n            <select class=\"form-control\" v-model=\"user_roles\">\n                <option>Business Development Officer</option>\n                <option>Business Development Executive Officer</option>\n                <option>Pension Administrator</option>\n                <option>Accountant</option>\n                <option>Fund Accountant</option>\n                <option>CRM</option>\n                <option>Senior Administrator</option>\n                <option>Head of Scheme & Administration</option>\n                <option>HR</option>\n                <option>Senior Pension Administrator</option>\n                <option>E-learning Administrator</option>\n                <option>Content Creator</option>\n                <option>Sys Admin</option>\n\n            </select>\n            <span>Choice: {{ user_roles }}</span>\n            </base-input>\n          </b-col>\n          <br>  \n          </b-row>      \n         <b-row>\n        <b-col lg=\"6\" sm=\"12\" xl=\"6\">   \n           <h4>Select Role Right(s)</h4>  \n        <b-row>\n          <b-col lg=\"3\" sm=\"3\">\n              <label class=\"form-checkbox\" for=\"admin\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"admin\" v-model=\"adminPicked\" true-value=\"1\" false-value=\"0\">\n               Admin\n              </label>\n          </b-col>\n          <b-col lg=\"4\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"approver\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"approver\" v-model=\"approverPicked\" true-value=\"1\" false-value=\"0\">\n               Approver\n              </label>\n          </b-col> \n          <b-col lg=\"4\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"supervisor\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"supervisor\" v-model=\"supervisorPicked\" true-value=\"1\" false-value=\"0\">\n               Supervisor\n              </label> \n          </b-col>\n          <b-col lg=\"3\" sm=\"3\">\n               <label class=\"form-checkbox\" for=\"member\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"member\" v-model=\"memberPicked\" true-value=\"1\" false-value=\"0\">\n              Member\n              </label>\n          </b-col>\n        </b-row>\n          </b-col>\n          <!--Access Zones -->\n          <b-col lg=\"6\" xl =\"6\" sm =\"12\">\n            <base-input\n              label=\"Zone\"\n            >\n            <p>Select your Zones</p>\n                        <!-- <select v-model=\"selected\">\n                          <option disabled value=\"\">Zone</option>\n                          <option\n                            :value=\"count.module_code\"\n                            v-for=\"count in oafslModules\"\n                            v-bind:key=\"count.module_code\"\n                          >\n                            {{ count.name }}\n                          </option>\n                        </select> -->\n       <select\n      multiple=\"true\"\n      v-bind:class=\"{ 'fix-height': multiple === 'true' }\"\n      v-model=\"multipleSelections\"\n      >\n    <option\n      :value=\"count.module_code\"\n       v-for=\"count in oafslModules\"\n       v-bind:key=\"count.module_code\"\n       >\n      {{count.name}}\n    </option>\n  </select>\n<!-- {{ multipleSelections }} -->\n            <!-- <select v-model=\"user_zone\" class=\"form-control\">\n              \n                <option>Master Setup</option>\n                <option>BD</option>\n                <option>Onboarding</option>\n                <option>CRM</option>\n                <option>Servicing</option>\n                 \n                <option>Accounting</option>\n                <option>Governance</option>\n                <option>Insurance</option>\n                <option>Election</option>\n                <option>Reports</option>\n                <option>Document centre</option>\n                <option>E-learning</option>\n                <option>Communication</option>\n                <option>E-board</option>\n                <option>Fund management</option>\n            </select> -->\n            <!-- <span>Choice: {{ multipleSelections }}</span> -->\n            </base-input>\n          </b-col>\n         </b-row>\n         <b-row>\n       \n          <b-col lg=\"6\">\n            <base-input\n              label=\"Access to\"\n            >\n           <select v-model=\"user_accessto\" class=\"form-control\">\n                <option v-if=\"multipleSelections ==='MSTP01'\">Master-setup - Product Setting</option>\n                <option v-if=\"multipleSelections ==='MSTP01'\">Master-setup - Module setting</option>\n                <option v-if=\"multipleSelections ==='MSTP01'\">Master-setup - User rights</option>\n                <option v-if=\"multipleSelections ==='MSTP01'\">Master-setup - (All Access)</option>\n                <option v-if=\"multipleS