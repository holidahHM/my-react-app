v-if=\"multipleSelections ==='Servicing'\">Servicing - (All Access)</option>\n            </select>\n            <span>Choice: {{ user_accessto }}</span>\n            </base-input>\n          </b-col>\n          <b-col lg=\"6\">\n            <base-input\n              label=\"User Type\"\n            >\n           <select class=\"form-control\" v-model=\"user_type\">\n                <option>Internal</option>\n            </select>\n            <span>Choice: {{ user_type }}</span>\n            </base-input>\n          </b-col>\n        </b-row>\n        <b-row>\n               <b-col lg=\"12\" sm=\"12\" xl=\"12\">\n                 <h4>Select Scheme</h4>\n           <div style=\"overflow: scroll;\"> \n\t  <b-row>\n\t  <b-col xl=\"12\" lg=\"12\" sm=12>\n    <span>Choice: {{ selected }}</span>\n<table class=\"table table-stripped\">\n\t    <thead class=\"thead-light\">\n\t      <tr>\n\t\t<th scope=\"col\">\n\t\t\t<label class=\"form-checkbox\">\n\t\t\t<input type='checkbox' v-model='selectAll' @click='select'> \n\t\t\t</label>\n\t\t\t</th>\n\t      <th scope=\"col\">Scheme Id</th>\n\t      <th scope=\"col\">Scheme Code</th >\n\t\t\t <th scope=\"col\">Scheme Name</th>\n\t\t\t <th scope=\"col\">Country</th>\n       <th scope=\"col\">Action</th>\n\n\t      </tr>\n\t    </thead>\n       <tbody  id=\"myTable\">\n\t      \t      \t<tr v-for=\"i in items\">\n\t\t\t\t<td>\n\t\t\t\t\t\t<label class=\"form-checkbox\" for=\"schemes\">\n    \t\t\t\t\t<input type=\"checkbox\" id=\"schemes\" :value=\"i.scheme_code \" v-model=\"selected\">\n\t\t\t\t\t\t<i class=\"form-icon\"></i>\n  \t\t\t\t\t</label>\n\t\t\t\t</td>\n\t      \t\t<td>{{i.scheme_id}}</td>\n\t      \t\t<td>{{i.scheme_code}}</td>\n\t      \t\t<td>{{i.scheme_name}}</td>\n\t      \t\t<td>{{i.scheme_country}}</td>\n\n<td>\n\t<b-dropdown id=\"dropdown-left\" text=\"Action\" variant=\"primary\" class=\"m-2\">\n    <b-dropdown-item href=\"#\">Open</b-dropdown-item>\n    <b-dropdown-item href=\"#\">Edit</b-dropdown-item>\n    