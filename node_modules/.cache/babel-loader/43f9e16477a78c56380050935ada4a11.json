mit }\" ref=\"formValidator\">\n                <b-form\n                  role=\"form\"\n                  method=\"post\"\n                  @submit.prevent=\"handleSubmit(onSubmit)\"\n                >\n                  <base-input\n                    alternative\n                    class=\"mb-3\"\n                    prepend-icon=\"ni ni-hat-3\"\n                    placeholder=\"Name\"\n                    name=\"Name\"\n                    :rules=\"{ required: true }\"\n                    v-model=\"model.username\"\n                  >\n                  </base-input>\n\n                  <base-input\n                    alternative\n                    class=\"mb-3\"\n                    prepend-icon=\"ni ni-email-83\"\n                    placeholder=\"Email\"\n                    name=\"Email\"\n                    :rules=\"{ required: true, email: true }\"\n                    v-model=\"model.user_email\"\n                  >\n                  </base-input>\n\n                  <base-input\n                    alternative\n                    class=\"mb-3\"\n                    prepend-icon=\"ni ni-lock-circle-open\"\n                    placeholder=\"password\"\n                    type=\"password\"\n                    name=\"Password\"\n                    :rules=\"{ required: true, min: 6 }\"\n                    v-model=\"model.password\"\n                  >\n                  </base-input>\n                  <div class=\"text-muted font-italic\">\n                    <small\n                      >password strength:\n                      <span class=\"text-success font-weight-700\">strong</span></small\n                    >\n                  </div>\n                  <b-row class=\"my-4\">\n                    <b-col cols=\"12\">\n                      <base-input\n                        :rules=\"{ required: { allowFalse: false } }\"\n                        name=\"Privacy\"\n                        Policy\n                      >\n                        <b-form-checkbox v-model=\"model.agree\">\n                          <span class=\"text-muted\"\n                            >I agree with the <a href=\"#!\">Privacy Policy</a></span\n                          >\n                        </b-form-checkbox>\n                      </base-input>\n                    </b-col>\n                  </b-row>\n                  <div class=\"text-center\">\n                    <b-button\n                      type=\"submit\"\n                      variant=\"primary\"\n                      class=\"mt-4\"\n                      @click=\"saveSystemUser\"\n                      >Create account</b-button\n                    >\n                  </div>\n                </b-form>\n              </validation-observer>\n            </b-card-body>\n          </b-card>\n        </b-col>\n      </b-row>\n    </b-container>\n  </div>\n</template>\n<script>\nimport { saveUser } from \"../../api/users.api\";\nexport default {\n  name: \"register\",\n  data() {\n    return {\n      model: {\n        username: \"\",\n        user_email: \"\",\n        password: \"\",\n        agree: false,\n      },\n    };\n  },\n  created() {\n    this.model.username = this.$route.params.details.p_name;\n    this.model.user_email = this.$route.params.details.p_email;\n  },\n  methods: {\n    addItem() {\n      this.items.push({\n        entity: \"\",\n      });\n    },\n    async saveSystemUser() {\n      this.error = \"\";\n      let response = \"\";\n      try {\n        this.$loading.show({ delay: 0 });\n        response = await saveUser(\n          this.model.username,\n          this.model.user_email,\n          this.model.password\n        );\n        console.log(response.data);\n      } catch (e) {\n        this.$loading.hide();\n        let respmess = insuranceErrors.getErrorMessages(e.response.status);\n        this.error = respmess;\n        throw respmess;\n      }\n      this.$loading.hide();\n      if (response.status === 200) {\n        alert(\"data saved successfully\");\n      }\n    },\n  },\n};\n</script>\n<style></style>\n"],"sourceRoot":"src/views/Pages"}]}